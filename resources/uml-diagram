           v
+----------------------+
|     VendingMachine   |
+----------------------+
| - state: VendingMachineState      |
| - inventory: Inventory            |
| - cashManager: CashManager        |
| - productCode: int                |
| - totalAmount: int                |
+----------------------+
| + getInstance(): VendingMachine   |
| + getState(): VendingMachineState |
| + setState(state): void           |
| + getInventory(): Inventory       |
| + addNotes(notes): void           |
| + resetVendingMachine(): void     |
| + getProductCode(): int           |
| + setProductCode(code): void      |
| + getTotalAmount(): int           |  -----------------------------
| + setTotalAmount(amount): void    |                              |
| + getNotes(): Map<Note, Integer>  |                              |
+----------------------+                                           V
                                                        +---------------------------------------+
                |                                        |              CashManager             |
                |                                        +--------------------------------------+
                |                                        | - cashBox: Map<Note, Integer>        |
                |                                        +---------------------------------------+
                |                                        | + addNotes(List<Note>)               |
                |                                        | + getTotalInsertedAmount(List<Note>) |
                |                                        | + hasSufficientChange(int): boolean  |
                |                                        | + getChange(int): Map<Note, Integer> |
                |                                        | + reset()                            |
                |                                        | + getCashBox(): Map<Note, Integer>   |
                |                                        +-------------------------------------+
                |
